{
  "$schema": "https://static.modelcontextprotocol.io/schemas/2025-12-11/server.schema.json",
  "name": "io.github.kcw2034-sayouzone/mcp-opendart-server",
  "description": "An MCP server that provides OpenDart API",
  "repository": {
    "url": "https://github.com/sayouzone/mcp-opendart-server",
    "source": "github"
  },
  "version": "0.0.4",
  "packages": [
    {
      "registryType": "pypi",
      "identifier": "sayou-opendart-mcp",
      "version": "0.0.1",
      "transport": {
        "type": "stdio"
      },
      "environmentVariables": [
        {
          "description": "Your API key for OpenDart",
          "isRequired": true,
          "format": "string",
          "isSecret": true,
          "name": "OPENDART_API_KEY"
        }
      ]
    }
  ]
}